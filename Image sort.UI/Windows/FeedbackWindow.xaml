<mahapps:MetroWindow x:Class="Image_sort.UI.Windows.FeedbackWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Image_sort.UI.Windows"
        xmlns:web="clr-namespace:Microsoft.Toolkit.Wpf.UI.Controls;assembly=Microsoft.Toolkit.Wpf.UI.Controls.WebView"
        xmlns:mahapps="http://metro.mahapps.com/winfx/xaml/controls"   
        xmlns:res="clr-namespace:Image_sort.UI.LocalResources.AppResources"
        mc:Ignorable="d"
        Title="Feedback" Height="600" Width="800">
    <Window.Resources>
        <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Accents/BaseLight.xaml" />
    </Window.Resources>
    <mahapps:MetroWindow.RightWindowCommands>
        <mahapps:WindowCommands>
            <Button Click="OnOpenInBrowserClicked" 
                    Content="{x:Static res:AppResources.OpenInBrowser}"/>
        </mahapps:WindowCommands>
    </mahapps:MetroWindow.RightWindowCommands>
    <web:WebViewCompatible Source="https://docs.google.com/forms/d/e/1FAIpQLSeRLmo5uw0ZTqrgFAYqVE5Wyfthh_BeSCCG19FYmhADwiSRcw/viewform">
        
    </web:WebViewCompatible>
</mahapps:MetroWindow>
