<reactiveui:ReactiveUserControl x:Class="ImageSort.WPF.Views.ActionsView"
                                x:TypeArguments="vm:ActionsViewModel"
                                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                                xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
                                xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                                xmlns:local="clr-namespace:ImageSort.WPF.Views"
                                xmlns:vm="clr-namespace:ImageSort.ViewModels;assembly=ImageSort"
                                xmlns:reactiveui="http://reactiveui.net"
                                mc:Ignorable="d" 
                                d:DesignWidth="800">
    <StackPanel Orientation="Horizontal">
        <Button x:Name="Undo" Content="Undo" Padding="5"/>
        <Button x:Name="Redo" Content="Redo" Padding="5"/>
    </StackPanel>
</reactiveui:ReactiveUserControl>
